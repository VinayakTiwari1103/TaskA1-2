{
  "name": "interview-scheduler",
  "version": "1.0.0",
  "description": "AI-powered interview scheduling system using Temporal workflows",
  "main": "dist/client.js",
  "scripts": {
    "build": "tsc",
    "worker": "cross-env NODE_OPTIONS=--no-deprecation npx tsx src/worker.ts",
    "start": "tsx src/client.ts",
    "dev": "concurrently \"npm run worker\" \"npm run start\"",
    "temporal:server": "docker-compose up -d",
    "cli": "cross-env NODE_OPTIONS=--no-deprecation npx tsx src/cli.ts"
  },
  "dependencies": {
    "@temporalio/client": "^1.12.1",
    "@temporalio/worker": "^1.8.0",
    "@temporalio/workflow": "^1.8.0",
    "@types/uuid": "^10.0.0",
    "axios": "^1.11.0",
    "chrono-node": "^2.8.3",
    "express": "^4.18.2",
    "googleapis": "^126.0.1",
    "node-fetch": "^3.3.2",
    "openai": "^5.10.1",
    "together-ai": "^0.20.0",
    "tsx": "^3.12.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.19.8",
    "concurrently": "^8.2.0",
    "cross-env": "^7.0.3",
    "typescript": "^5.1.6"
  }
}
